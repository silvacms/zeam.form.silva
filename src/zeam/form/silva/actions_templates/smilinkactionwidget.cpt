<a class="ui-state-default form-control link-control ${widget.css_class}"
   id="${widget.htmlId()}"
   name="${widget.identifier}"
   target="${widget.target}"
   title="${widget.description}"
   data-form-shortcut="${widget.accesskey}">
  <ins class="ui-icon ${widget.icon}"></ins>
  <tal:label tal:content="widget.title">Label</tal:label>
</a>
