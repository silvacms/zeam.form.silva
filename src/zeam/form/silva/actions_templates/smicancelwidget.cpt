<a class="ui-state-default form-button open-screen"
   id="${widget.htmlId()}"
   rel="${widget.screen}"
   title="${widget.description}"
   href="${widget.contentPath}"
   data-form-shortcut="${widget.htmlAttribute('accesskey')}">
  <ins class="ui-icon ui-icon-arrowreturnthick-1-w"></ins>
  <tal:label tal:content="widget.title">Label</tal:label>
</a>
