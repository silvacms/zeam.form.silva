<a class="ui-state-default form-control ${widget.css_class}"
   id="${widget.htmlId()}"
   name="${widget.identifier}"
   title="${widget.description}"
   data-form-shortcut="${widget.htmlAttribute('accesskey')}"
   data-confirmation="${widget.htmlAttribute('data-confirmation')}">
  <ins class="${widget.icon}"></ins>
  <tal:label tal:content="widget.title">Label</tal:label>
</a>
